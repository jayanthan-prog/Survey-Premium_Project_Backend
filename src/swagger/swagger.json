      "get": {
        "summary": "List users",
        "tags": ["Users"],
        "responses": {
          "200": {
            "description": "A list of users",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "user_id": { "type": "string", "format": "uuid" },
                      "name": { "type": "string" },
                      "email": { "type": "string", "format": "email" },
                      "created_at": { "type": "string", "format": "date-time", "nullable": true }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
